zaroo
    NOP
    NoP
    noP

start:
    LDIB 0
    STAB ledloc
    LDIB 0ffh 
    STAB ledloc
    JMP start

ledloc .equ $123

    LDAB @X
    LDAB @X, 0
    LDAB @X, 19
